{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run Jetty Server",
            "type": "shell",
            "command": "mvn clean jetty:run -P watch-all",
            "group": {
               "kind": "build",
               "isDefault": true
            }
        },
        {
            "label": "Run stand-alone Jetty Server Alternative ohne Anf√ºhrungszeichen",
            "type": "shell",
            "command": "mvn clean jetty:run -P watch-all,include-mapapps-deps",
            "group": {
               "kind": "build",
               "isDefault": true
            }
        },
        {
            "label": "Run stand-alone Jetty Server",
            "type": "shell",
            "command": "mvn clean jetty:run -P 'watch-all,include-mapapps-deps'",
            "group": {
               "kind": "build",
               "isDefault": true
            }
        },
        {
            "label": "Install",
            "type": "shell",
            "command": "mvn clean install -P compress",
            "group": {
               "kind": "build",
               "isDefault": true
            }
        }
    ]
